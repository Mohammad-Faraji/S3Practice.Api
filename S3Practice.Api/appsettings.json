{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",


  "S3Configuration": {
    "AccessKey": "YOUR_ACCESS_KEY",
